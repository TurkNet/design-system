{
  "name": "@turk.net/design-system",
  "version": "0.8.1",
  "repository": "https://github.com/TurkNet/design-system.git",
  "author": "Recep Köseoğlu <recepkoseoglu@hotmail.com>",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "start": "start-storybook -p 6006 -s public",
    "build": "build-storybook -s public",
    "build-library": "rm -rf dist && tsc --project tsconfig.library.json && yarn copypackage",
    "copypackage": "cp -rf .npmrc package.json LICENSE README.md dist",
    "clean": "rm -rf node_modules dist yarn.lock && yarn cache clean && yarn",
    "format": "prettier --write .",
    "lint": "tsc --noEmit && eslint . --fix",
    "chromatic": "chromatic --exit-zero-on-changes",
    "release": "./scripts/release.sh"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint",
      "yarn format",
      "git add ."
    ],
    "package.json": "sort-package-json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9"
  },
  "dependencies": {
    "@types/react-datepicker": "4.3.3",
    "@types/react-slick": "0.23.5",
    "@types/styled-system": "5.1.11",
    "color": "3.1.3",
    "date-fns": "2.27.0",
    "react-datepicker": "4.5.0",
    "react-select": "5.2.1",
    "react-slick": "0.28.1",
    "styled-components": "5.2.1",
    "styled-system": "5.1.5",
    "styled-tools": "1.7.2"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "6.4.13",
    "@storybook/addon-actions": "6.4.13",
    "@storybook/addon-essentials": "6.4.13",
    "@storybook/addon-links": "6.4.13",
    "@storybook/addon-storysource": "6.4.13",
    "@storybook/node-logger": "6.4.13",
    "@storybook/preset-create-react-app": "3.2.0",
    "@storybook/react": "6.4.13",
    "@types/color": "3.0.1",
    "@types/react": "17.0.24",
    "@types/react-dom": "17.0.9",
    "@types/styled-components": "5.1.9",
    "@types/styled-system": "5.1.11",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "auto": "10.25.1",
    "chromatic": "5.6.2",
    "eslint": "7.24.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.3.5",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "1.7.0",
